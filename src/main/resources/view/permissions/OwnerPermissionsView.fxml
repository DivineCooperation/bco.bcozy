<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.HBox?>
<?import org.openbase.bco.bcozy.view.ObserverLabel?>
<HBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.openbase.bco.bcozy.permissions.OwnerPermissionsController"
      spacing="10">

    <ObserverLabel labelFor="${owner}">permissions.owner</ObserverLabel>

    <ChoiceBox fx:id="owner"/>

    <ObserverLabel labelFor="${read}">readRight</ObserverLabel>
    <CheckBox fx:id="read"/>

    <ObserverLabel labelFor="${write}">writeRight</ObserverLabel>
    <CheckBox fx:id="write"/>

    <ObserverLabel labelFor="${access}">accessRight</ObserverLabel>
    <CheckBox fx:id="access"/>
</HBox>
