<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.layout.VBox?>
<?import org.controlsfx.control.textfield.CustomTextField?>
<AnchorPane fx:id="settingsPaneParent" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.openbase.bco.bcozy.controller.SettingsController">
    <children>
        <TabPane fx:id="tabPane" layoutX="3.0" layoutY="4.0" tabClosingPolicy="UNAVAILABLE"
                 AnchorPane.bottomAnchor="0.0"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="1.0">
            <tabs>
                <Tab fx:id="settingsTab">
                    <content>
                    </content>
                </Tab>
                <Tab fx:id="permissionTab">
                    <content>
                        <VBox fx:id="permissionPaneParent" minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                              prefWidth="200.0">
                            <children>
                                <CustomTextField fx:id="filterInput" layoutX="433.0" layoutY="2.0" prefHeight="13.0"
                                                 prefWidth="161.0"></CustomTextField>
                                <JFXTreeTableView fx:id="unitsTable" AnchorPane.bottomAnchor="300.0"
                                                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                                  AnchorPane.topAnchor="0.0"/>

                            </children>
                        </VBox>
                    </content>
                </Tab>
                <Tab fx:id="registrationTab">
                    <content>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
